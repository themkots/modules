set mp $modpath.3
set mpre [regsub -all "\(\[.+?\]\)" $mp {\\\1}]

# setup specific environment
setenv_path_var MODULEPATH $mp
setenv_path_var MODULES_ADVANCED_VERSION_SPEC 0


# multiple tests of 'versioncmp' command are shipped in versioncmp/.modulerc
# and versioncmp/1.0
set ans [list]
lappend ans [list setpath LOADEDMODULES versioncmp/1.0]
lappend ans [list setpath _LMFILES_ $mp/versioncmp/1.0]
testouterr_cmd sh {load versioncmp/1.0} $ans {}


#
#  Cleanup
#

reset_test_env
